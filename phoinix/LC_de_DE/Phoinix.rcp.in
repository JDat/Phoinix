FORM ID formIdNoGame AT (0 0 160 160)
  MENUID menuIdNoGame
BEGIN
  TITLE "Phoinix"

  LABEL "Keine Spiele gefunden."             AUTOID AT (CENTER@80 40) FONT 2
  LABEL "Wegen Links auf freie Spiele siehe" AUTOID AT (CENTER@80 80)
  LABEL "phoinix.sourceforge.net"            AUTOID AT (CENTER@80 PREVBOTTOM)
END

MENU ID menuIdNoGame
BEGIN
  PULLDOWN "Optionen"
  BEGIN
    MENUITEM "Tipps..."        ID menuIdNoGameOptionsTips
    MENUITEM "Über Phoinix..." ID menuIdNoGameOptionsAbout
  END
END

STRING ID helpIdNoGame
  "Phoinix emuliert nur die Hardware des Gameboys, ein Spiel zu spielen "\
  "erfordert dessen Binärdaten. Sie finden diese als Dateien, "\
  "üblicherweise mit einer \"*.GB\" Dateierweiterung.\n"\
  "\n"\
  "Die erhaltene Spieledatei muss in eine Palm-Datenbank (\"*.PDB\") "\
  "konvertiert werden. Das macht der Konverter \"gb2pdb\" auf Ihrem PC. "\
  "Bitte lesen Sie die Dokumentation!"

//=== Game Manager ========================================================

ALERT ID alertIdManagerListError
  ERROR
BEGIN
  TITLE "Applikationsfehler"
  MESSAGE "Die Spieleliste konnte nicht erzeugt werden."
  BUTTONS "OK"
END

ALERT ID alertIdManagerListIncomplete
  ERROR
BEGIN
  TITLE "Applikationsfehler"
  MESSAGE "Wegen Speicherproblemen ist die Spieleliste unvollständig."
  BUTTONS "OK"
END

FORM ID formIdManager AT (0 0 160 160)
  MENUID menuIdManager
BEGIN
  TITLE "Phoinix"

  POPUPTRIGGER "" ID popupIdManagerName AT (0 17 0 AUTO) LEFTANCHOR
  LIST "" ID listIdManagerName
    AT (PREVRIGHT+13 PREVTOP 160-(PREVRIGHT+13)-2 0) NONUSABLE
  FIELD ID fieldIdManagerName
    AT (PREVLEFT+4 PREVTOP 160-(PREVLEFT+4)-2 AUTO) FONT 1
    UNDERLINED MAXCHARS dmDBNameLength-1 AUTOSHIFT
  POPUPLIST ID popupIdManagerName listIdManagerName

  LABEL "Ort:" labelIdManagerLocation AT (RIGHT@55 32) FONT 1
  POPUPTRIGGER "" ID popupIdManagerLocation 
    AT (PREVRIGHT+5 PREVTOP AUTO AUTO) LEFTANCHOR
  LIST "" ID listIdManagerLocation
    AT (PREVRIGHT PREVTOP 160-PREVRIGHT-2 AUTO) NONUSABLE
  POPUPLIST ID popupIdManagerLocation listIdManagerLocation

  LABEL "Typ:" AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(30 chars)--------------------" labelIdManagerType
    AT (PREVRIGHT+5 PREVTOP)
  LABEL "ROM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRom AT (PREVRIGHT+5 PREVTOP)
  LABEL "RAM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRam AT (PREVRIGHT+5 PREVTOP)

  BUTTON "LOS!" ID buttonIdManagerGo
    AT (CENTER@80 BOTTOM@160-1 80+2 72+2) FONT 2

  GRAFFITISTATEINDICATOR AT (160-10 160-(72+2)-15)
END

MENU ID menuIdManager
BEGIN
  PULLDOWN "Aktionen"
  BEGIN
    MENUITEM "Spiel löschen..." ID menuIdManagerActionsDeleteGame
  END

  PULLDOWN "Bearb."
  BEGIN
    MENUITEM "Rückgängig"      ID sysEditMenuUndoCmd      "U"
    MENUITEM "Ausschneiden"    ID sysEditMenuCutCmd       "X"
    MENUITEM "Kopieren"        ID sysEditMenuCopyCmd      "C"
    MENUITEM "Einfügen"        ID sysEditMenuPasteCmd     "V"
    MENUITEM "Alles auswählen" ID sysEditMenuSelectAllCmd "A"
    MENUITEM SEPARATOR
    MENUITEM "Tastatur"        ID sysEditMenuKeyboardCmd  "T"
    MENUITEM "Graffiti-Hilfe"  ID sysEditMenuGraffitiCmd  "G"
  END

  PULLDOWN "Optionen"
  BEGIN
    MENUITEM "Tastenzuordnung..." ID menuIdManagerOptionsButtonMapping
    MENUITEM SEPARATOR
    MENUITEM "Tipps..."           ID menuIdManagerOptionsTips
    MENUITEM "Über Phoinix..."    ID menuIdManagerOptionsAbout
  END
END

STRING ID helpIdManager
  "Sie wählen das gewünschte Spiel auf eine von zwei Arten aus:\n"\
  "\x95 Benutzen Sie das Popup, um aus der Liste zu wählen.\n"\
  "\x95 Drücken Sie die Tasten für den Bildlauf.\n"\
  "\n"\
  "Zum Start der Emulation tippen Sie auf \"LOS!\" und das Spiel läuft "\
  "los, als ob der Gameboy gerade eingeschaltet wurde.\n"\
  "\n"\
  "Sie können das Spiel nach Belieben umbenennen. Um eine Eingabe "\
  "abzubrechen, wählen Sie das Spiel in der Popup-Liste einfach nochmal "\
  "aus."

