//=== Emulation ===========================================================

FORM ID formIdEmulation AT (0 0 160 160)
  MENUID menuIdEmulation
BEGIN
  BUTTON "Select" ID buttonIdEmulationSelect
    AT (RIGHT@80-6 BOTTOM@159 30 AUTO) DISABLED
  BUTTON "Start"  ID buttonIdEmulationStart
    AT (80+6       BOTTOM@159 30 AUTO) DISABLED

  POPUPTRIGGER "" ID popupIdEmulationInfo
    AT (120 BOTTOM@159 54 AUTO) LEFTANCHOR
  /* NOTE: the texts have to match the EmulationInfoType in emulation.h! */
  LIST "Tiempo jugado" "Batería" "Velocidad" ID listIdEmulationInfo
    AT (PREVLEFT+4 PREVTOP AUTO AUTO) NONUSABLE VISIBLEITEMS 3
  POPUPLIST ID popupIdEmulationInfo listIdEmulationInfo

  CHECKBOX "Exec." ID checkboxIdEmulationRun AT (0 BOTTOM@159 AUTO AUTO)
END

// This form is for GB screen's bounds only, not to be shown.
FORM ID formIdEmulationScreen AT (0 0 240 240)
BEGIN
  GADGET ID gadgetIdEmulationSmall AT (0 0   160 144)
  GADGET ID gadgetIdEmulationH330  AT (40 36 160 144)
END

MENU ID menuIdEmulation
BEGIN
  PULLDOWN "Juego"
  BEGIN
    MENUITEM "Salir"    ID menuIdEmulationGameQuit
    MENUITEM SEPARATOR
    MENUITEM "Resetear" ID menuIdEmulationGameReset
  END

  PULLDOWN "Opciones"
  BEGIN
    MENUITEM "Mapeamiento de Botones..." ID menuIdEmulationOptionsButtonMapping
    MENUITEM SEPARATOR
    MENUITEM "Sugerencias..."            ID menuIdEmulationOptionsTips
    MENUITEM "Acerca de Phoinix..."      ID menuIdEmulationOptionsAbout
  END
END

STRING ID helpIdEmulation
  "Para reiniciar un juego como si estuviera apagando y encendiendo un "\
  "Gameboy de verdad, use \"Resetear\".\n"\
  "\n"\
  "Para parar un juego momentáneamente, desmarque \"Executar\". Ésto es "\
  "ideal para la captura de pantalla."

ALERT ID alertIdEmulationSetupError
