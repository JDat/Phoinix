BEGIN
  PULLDOWN "Jeux"
  BEGIN
    MENUITEM "Quitter"    ID menuIdEmulationGameQuit
    MENUITEM SEPARATOR
    MENUITEM "Redémarrer" ID menuIdEmulationGameReset
