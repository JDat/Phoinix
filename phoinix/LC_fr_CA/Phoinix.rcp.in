    MENUITEM "Arrangement des boutons..." ID menuIdEmulationOptionsButtonMapping
    MENUITEM SEPARATOR
    MENUITEM "Conseils..."                ID menuIdEmulationOptionsTips
    MENUITEM "À propos de Phoinix..."     ID menuIdEmulationOptionsAbout
  END
END

STRING ID helpIdEmulation
  "Pour remettre en marche un jeux, faite comme si vous auriez un vrai "\
  "Gameboy, utiliser \"Redémarrer\".\n"\
  "\n"\
  "Pour arrêter un jeux momentanément, décoché la case \"Exéc.\". "\
  "C'est pratique pour les photos d'écran."

