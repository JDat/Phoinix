MENU ID menuIdNoGame
BEGIN
  PULLDOWN "Options"
  BEGIN
    MENUITEM "Conseils..."            ID menuIdNoGameOptionsTips
    MENUITEM "À propos de Phoinix..." ID menuIdNoGameOptionsAbout
  END
END

STRING ID helpIdNoGame
  "Phoinix émule seulement le côté matériel du Gameboy, jouer à un jeux "\
  "demande le fichier binaire. Vous les trouvez habituellement sous le "\
  "format d'un fichier ayant \"*.GB\" commet extensions.\n"\
  "\n"\
  "Après avoir obtenu un jeux, il doit être converti en fichier "\
  "compréhensible par un Palm (\"*.PDB\"). Cette opération peut être "\
  "effectuer à l'aide du convertiseur \"gb2pdb\" sur votre ordinateur "\
  "personnel. SVP, lisez la documentation!"

//=== Game Manager ========================================================

ALERT ID alertIdManagerListError
  ERROR
BEGIN
  TITLE "Erreur d'application"
  MESSAGE "La liste de jeux ne peut être créer."
  BUTTONS "OK"
END

ALERT ID alertIdManagerListIncomplete
  ERROR
BEGIN
  TITLE "Erreur d'application"
  MESSAGE "Dû à des problèmes de mémoire, la liste de jeux est incomplète."
  BUTTONS "OK"
END

FORM ID formIdManager AT (0 0 160 160)
  MENUID menuIdManager
BEGIN
  TITLE "Phoinix"

  POPUPTRIGGER "" ID popupIdManagerName AT (0 17 0 AUTO) LEFTANCHOR
  LIST "" ID listIdManagerName
    AT (PREVRIGHT+13 PREVTOP 160-(PREVRIGHT+13)-2 0) NONUSABLE
  FIELD ID fieldIdManagerName
    AT (PREVLEFT+4 PREVTOP 160-(PREVLEFT+4)-2 AUTO) FONT 1
    UNDERLINED MAXCHARS dmDBNameLength-1 AUTOSHIFT
  POPUPLIST ID popupIdManagerName listIdManagerName

  LABEL "Location:" labelIdManagerLocation AT (RIGHT@55 32) FONT 1
  POPUPTRIGGER "" ID popupIdManagerLocation
    AT (PREVRIGHT+5 PREVTOP AUTO AUTO) LEFTANCHOR
  LIST "" ID listIdManagerLocation
    AT (PREVRIGHT PREVTOP 160-PREVRIGHT-2 AUTO) NONUSABLE
  POPUPLIST ID popupIdManagerLocation listIdManagerLocation

  LABEL "Type:" AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(30 chars)--------------------" labelIdManagerType
    AT (PREVRIGHT+5 PREVTOP)
  LABEL "ROM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRom AT (PREVRIGHT+5 PREVTOP)
  LABEL "RAM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRam AT (PREVRIGHT+5 PREVTOP)

  BUTTON "GO!" ID buttonIdManagerGo
    AT (CENTER@80 BOTTOM@160-1 80+2 72+2) FONT 2

  GRAFFITISTATEINDICATOR AT (160-10 160-(72+2)-15)
END

MENU ID menuIdManager
BEGIN
  PULLDOWN "Actions"
  BEGIN
    MENUITEM "Supprimer jeux..." ID menuIdManagerActionsDeleteGame
  END

  PULLDOWN "Éditer"
  BEGIN
    MENUITEM "Annuler"           ID sysEditMenuUndoCmd      "Z"
    MENUITEM "Couper"            ID sysEditMenuCutCmd       "X"
    MENUITEM "Copier"            ID sysEditMenuCopyCmd      "C"
    MENUITEM "Coller"            ID sysEditMenuPasteCmd     "V"
    MENUITEM "Sélectionner tout" ID sysEditMenuSelectAllCmd "T"
    MENUITEM SEPARATOR
    MENUITEM "Clavier"           ID sysEditMenuKeyboardCmd  "A"
    MENUITEM "Aide Graffiti"     ID sysEditMenuGraffitiCmd  "G"
  END

  PULLDOWN "Options"
  BEGIN
    MENUITEM "Arrangement des boutons..." ID menuIdManagerOptionsButtonMapping
    MENUITEM SEPARATOR
    MENUITEM "Conseils..."                ID menuIdManagerOptionsTips
    MENUITEM "À propos de Phoinix..."     ID menuIdManagerOptionsAbout
  END
END

STRING ID helpIdManager
  "Sélectioner le jeux que vous voulez jouer d'une de ces deux façon:\n"\
  "\x95 Utiliser le menu de la liste pour sélectionner un jeux.\n"\
  "\x95 Appuyez sur la flèche.\n"\
  "\n"\
  "Pour démarrer l'émulation écriver \"GO!\" et le jeux commencera comme "\
  "si on aurait allumé le Gameboy.\n"\
  "\n"\
  "Vous pouvez renommer le jeux comme vous voulez. Pour annulez l'entrée "\
  "sélectionner de nouveau le jeux dans la liste."

