  MENUID menuIdNoGame
BEGIN
  TITLE "Phoinix"

  LABEL "Nessun Gioco Presente."   AUTOID AT (CENTER@80 40) FONT 2
  LABEL "Per ottenere links a"     AUTOID AT (CENTER@80 BOTTOM@80)
  LABEL "giochi freeware visitate" AUTOID AT (CENTER@80 PREVBOTTOM)
  LABEL "phoinix.sourceforge.net"  AUTOID AT (CENTER@80 PREVBOTTOM)
END

MENU ID menuIdNoGame
BEGIN
  PULLDOWN "Opzioni"
  BEGIN
    MENUITEM "Suggerimenti..."    ID menuIdNoGameOptionsTips
    MENUITEM "Info su Phoinix..." ID menuIdNoGameOptionsAbout
  END
END

STRING ID helpIdNoGame
  "Phoinix emula solo l'hardware del Gameboy, per poter giocare è "\
  "necessario avere i dati di un gioco in formato binario. E' possibile "\
  "trovare questi dati sotto forma di file che di solito hanno "\
  "estensione \"*.GB\".\n"\
  "\n"\
  "Dopo che si è ottenuto il file .GB del gioco, è necessario "\
  "convertirlo nel formato dati del Palm (\"*.PDB\"). Ciò viene fatto "\
  "tramite il convertitore \"gb2pdb\" sul vostro personal computer. Si "\
  "suggerisce caldamente di leggerne le istruzioni!"

//=== Game Manager ========================================================

ALERT ID alertIdManagerListError
  ERROR
BEGIN
  TITLE "Errore del programma"
  MESSAGE "Non è possibile creare la lista dei giochi."
  BUTTONS "OK"
END

ALERT ID alertIdManagerListIncomplete
  ERROR
BEGIN
  TITLE "Errore del programma"
  MESSAGE "La lista dei giochi è incompleta per un problema di memoria ."
  BUTTONS "OK"
END

FORM ID formIdManager AT (0 0 160 160)
  MENUID menuIdManager
BEGIN
  TITLE "Phoinix"

  POPUPTRIGGER "" ID popupIdManagerName AT (0 17 0 AUTO) LEFTANCHOR
  LIST "" ID listIdManagerName
    AT (PREVRIGHT+13 PREVTOP 160-(PREVRIGHT+13)-2 0) NONUSABLE
  FIELD ID fieldIdManagerName
    AT (PREVLEFT+4 PREVTOP 160-(PREVLEFT+4)-2 AUTO) FONT 1
    UNDERLINED MAXCHARS dmDBNameLength-1 AUTOSHIFT
  POPUPLIST ID popupIdManagerName listIdManagerName

  LABEL "Posizione:" labelIdManagerLocation AT (RIGHT@55 32) FONT 1
  POPUPTRIGGER "" ID popupIdManagerLocation 
    AT (PREVRIGHT+5 PREVTOP AUTO AUTO) LEFTANCHOR
  LIST "" ID listIdManagerLocation
    AT (PREVRIGHT PREVTOP 160-PREVRIGHT-2 AUTO) NONUSABLE
  POPUPLIST ID popupIdManagerLocation listIdManagerLocation

  LABEL "Tipo:" AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(30 chars)--------------------" labelIdManagerType
    AT (PREVRIGHT+5 PREVTOP)
  LABEL "ROM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRom AT (PREVRIGHT+5 PREVTOP)
  LABEL "RAM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRam AT (PREVRIGHT+5 PREVTOP)

  BUTTON "VIA!" ID buttonIdManagerGo
    AT (CENTER@80 BOTTOM@160-1 80+2 72+2) FONT 2

  GRAFFITISTATEINDICATOR AT (160-10 160-(72+2)-15)
END

MENU ID menuIdManager
BEGIN
  PULLDOWN "Azioni"
  BEGIN
    MENUITEM "Cancella Gioco..." ID menuIdManagerActionsDeleteGame
  END

  PULLDOWN "Modifica"
  BEGIN
    MENUITEM "Annulla"           ID sysEditMenuUndoCmd      "Z"
    MENUITEM "Taglia"            ID sysEditMenuCutCmd       "X"
    MENUITEM "Copia"             ID sysEditMenuCopyCmd      "C"
    MENUITEM "Incolla"           ID sysEditMenuPasteCmd     "V"
    MENUITEM "Seleziona Tutto"   ID sysEditMenuSelectAllCmd "A"
    MENUITEM SEPARATOR
    MENUITEM "Tastiera"          ID sysEditMenuKeyboardCmd  "T"
    MENUITEM "Guida di Graffiti" ID sysEditMenuGraffitiCmd  "G"
  END

  PULLDOWN "Opzioni"
  BEGIN
    MENUITEM "Config. Tasti..."   ID menuIdManagerOptionsButtonMapping
    MENUITEM SEPARATOR
    MENUITEM "Suggerimenti..."    ID menuIdManagerOptionsTips
    MENUITEM "Info su Phoinix..." ID menuIdManagerOptionsAbout
  END
END

STRING ID helpIdManager
  "Si possono selezionare i giochi in uno dei due seguenti modi:\n"\
  "\x95 Toccando la freccia sullo schermo compare una lista da cui è "\
  "possibile scegliere il gioco.\n"\
  "\x95 Premendo i pulsanti di scorrimento verticale si passa al gioco "\
  "successivo (o a quello precedente) della lista.\n"\
  "\n"\
  "Per iniziare l'emulazione toccare \"VIA!\" ed il gioco inizierà come "\
  "se si stesse usando un Gameboy.\n"\
  "\n"\
  "E' possibile cambiare nome ai giochi. Per annullare l'operazione si "\
  "deve semplicemente scegliere di nuovo il gioco dal menù a comparsa."

