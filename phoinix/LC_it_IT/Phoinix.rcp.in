//=== Emulation ===========================================================

FORM ID formIdEmulation AT (0 0 160 160)
  MENUID menuIdEmulation
BEGIN
  BUTTON "Select" ID buttonIdEmulationSelect
    AT (RIGHT@80-6 BOTTOM@159 30 AUTO) DISABLED
  BUTTON "Start"  ID buttonIdEmulationStart
    AT (80+6       BOTTOM@159 30 AUTO) DISABLED

  POPUPTRIGGER "" ID popupIdEmulationInfo
    AT (120 BOTTOM@159 54 AUTO) LEFTANCHOR
  /* NOTE: the texts have to match the EmulationInfoType in emulation.h! */
  LIST "Minuti di gioco" "Batteria" "Velocità" ID listIdEmulationInfo
    AT (PREVLEFT+4 PREVTOP AUTO AUTO) NONUSABLE VISIBLEITEMS 3
  POPUPLIST ID popupIdEmulationInfo listIdEmulationInfo

  CHECKBOX "Esegui" ID checkboxIdEmulationRun AT (0 BOTTOM@159 AUTO AUTO)
END

// This form is for GB screen's bounds only, not to be shown.
FORM ID formIdEmulationScreen AT (0 0 240 240)
BEGIN
  GADGET ID gadgetIdEmulationSmall AT (0 0   160 144)
  GADGET ID gadgetIdEmulationH330  AT (40 36 160 144)
END

MENU ID menuIdEmulation
BEGIN
  PULLDOWN "Gioco"
  BEGIN
    MENUITEM "Esci"    ID menuIdEmulationGameQuit
    MENUITEM SEPARATOR
    MENUITEM "Resetta" ID menuIdEmulationGameReset
  END

  PULLDOWN "Opzioni"
  BEGIN
    MENUITEM "Config. Tasti..."   ID menuIdEmulationOptionsButtonMapping
    MENUITEM SEPARATOR
    MENUITEM "Suggerimenti..."    ID menuIdEmulationOptionsTips
    MENUITEM "Info su Phoinix..." ID menuIdEmulationOptionsAbout
  END
END

STRING ID helpIdEmulation
  "Per resettare un gioco si deve selezionare \"Resetta\"; l'effetto è "\
  "analogo a ciò che accade quando un vero Gameboy viene spento e "\
  "riacceso.\n"\
  "\n"\
  "Per sospendere momentaneamente un gioco si deve deselezionare "\
  "\"Esegui\". Questa opzione può essere utile per gli screenshots."

ALERT ID alertIdEmulationSetupError
  ERROR
BEGIN
  TITLE "Errore nell'emulazione"
  MESSAGE "Non è possibile attuare l'emulazione.\n"\
          "\x95 Si controlli che sia disponibile una sufficiente "\
	  "quantità di memoria libera.\n"\
       	  "\x95 Attualmente Phoinix non può essere eseguito dalla ROM "\
	  "flash."
  BUTTONS "OK"
END

ALERT ID alertIdEmulationRunError
  ERROR
BEGIN
  TITLE "Errore nell'emulazione"
  MESSAGE "Si è verificato un errore. Si prega di contattare\n"\
          "palm.gb_emu@gmx.de\n"\
          "per ottenere aiuto. Descrivete ciò che è accaduto nei minimi "\
	  "dettagli. Certo che potete farlo!"
  BUTTONS "OK"
END

