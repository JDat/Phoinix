FORM ID formIdManager AT (0 0 160 160)
  MENUID menuIdManager
BEGIN
  TITLE "Phoinix"

  POPUPTRIGGER "" ID popupIdManagerName AT (0 17 0 AUTO) LEFTANCHOR
  LIST "" ID listIdManagerName
    AT (PREVRIGHT+13 PREVTOP 160-(PREVRIGHT+13)-2 0) NONUSABLE
  FIELD ID fieldIdManagerName
    AT (PREVLEFT+4 PREVTOP 160-(PREVLEFT+4)-2 AUTO) FONT 1
    UNDERLINED MAXCHARS dmDBNameLength-1 AUTOSHIFT
  POPUPLIST ID popupIdManagerName listIdManagerName

  LABEL "Location:" labelIdManagerLocation AT (RIGHT@55 32) FONT 1
  POPUPTRIGGER "" ID popupIdManagerLocation 
    AT (PREVRIGHT+5 PREVTOP AUTO AUTO) LEFTANCHOR
  LIST "" ID listIdManagerLocation
    AT (PREVRIGHT PREVTOP 160-PREVRIGHT-2 AUTO) NONUSABLE
  POPUPLIST ID popupIdManagerLocation listIdManagerLocation

  LABEL "Type:" AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(30 chars)--------------------" labelIdManagerType
    AT (PREVRIGHT+5 PREVTOP)
  LABEL "ROM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRom AT (PREVRIGHT+5 PREVTOP)
  LABEL "RAM:"  AUTOID AT (RIGHT@55 PREVBOTTOM) FONT 1
  LABEL "(15 chars)-----" labelIdManagerRam AT (PREVRIGHT+5 PREVTOP)

  BUTTON "開始!" ID buttonIdManagerGo
    AT (CENTER@80 BOTTOM@160-1 80+2 72+2) FONT 2

  GRAFFITISTATEINDICATOR AT (160-10 160-(72+2)-15)
END

MENU ID menuIdManager
BEGIN
  PULLDOWN "実行"
  BEGIN
    MENUITEM "ｹﾞｰﾑの削除" ID menuIdManagerActionsDeleteGame
  END

  PULLDOWN "編集"
  BEGIN
    MENUITEM "元に戻す"          ID sysEditMenuUndoCmd      "U"
    MENUITEM "切り取り"           ID sysEditMenuCutCmd       "X"
    MENUITEM "ｺﾋﾟｰ"          ID sysEditMenuCopyCmd      "C"
    MENUITEM "ﾍﾟｰｽﾄ"         ID sysEditMenuPasteCmd     "P"
    MENUITEM "全て選択"    ID sysEditMenuSelectAllCmd "S"
    MENUITEM SEPARATOR
    MENUITEM "ｷｰﾎﾞｰﾄﾞ"      ID sysEditMenuKeyboardCmd  "K"
    MENUITEM "Graffiti Help" ID sysEditMenuGraffitiCmd  "G"
  END

  PULLDOWN "ｵﾌﾟｼｮﾝ"
  BEGIN
    MENUITEM "ﾎﾞﾀﾝﾏｯﾋﾟﾝｸﾞ" ID menuIdManagerOptionsButtonMapping
    MENUITEM SEPARATOR
    MENUITEM "Tips"           ID menuIdManagerOptionsTips
    MENUITEM "Phoinixについて"  ID menuIdManagerOptionsAbout
  END
END

STRING ID helpIdManager
  "2つの方法から遊びたいｹﾞｰﾑを選択してください:\n"\
  " 1.ﾎﾟｯﾌﾟｱｯﾌﾟのﾘｽﾄの中から選択する。\n"\
  " 2.ｽｸﾛｰﾙﾎﾞﾀﾝを押す。\n"\
  "\n"\
  "ｴﾐｭﾚｰｼｮﾝを開始するには\"開始!\"をﾀｯﾌﾟします。\n"\
  "すると、ｹﾞｰﾑﾎﾞｰｲにｽｲｯﾁをいれたように、ｹﾞｰﾑがはじまります。\n"\
  "\n"\
  "あなたの使いやすいようにｹﾞｰﾑ名を変更できます。\n"\
  "ｴﾝﾄﾘｰをｷｬﾝｾﾙする場合は、ﾎﾟｯﾌﾟｱｯﾌﾟのﾘｽﾄから再度選択します。"

